{
  "server-name": "%%mlAppName%%",
  "server-type": "http",
  "enabled": "true",
  "root": "/",
  "port": %%mlRestPort%%,
  "content-database": "%%DATABASE%%",
  "modules-database": "%%MODULES_DATABASE%%",
  "authentication": "%%mlRestAuthentication%%",
  "internal-security": "true",
  "threads": %%mlRestMaxThreads%%,
  "max-time-limit": %%mlRestMaxTimeout%%,
  "default-time-limit": %%mlRestDefaultTimeout%%,
  "url-rewriter": "/MarkLogic/rest-api/rewriter.xml",
  "rewrite-resolves-globally": true,
  "error-handler": "/customErrorHandler.mjs",
  "default-user": "nobody"
}
